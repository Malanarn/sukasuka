#include <iostream>
#include <fstream>
using namespace std;

int main(){
	
	//open an output file for spooling
	ofstream outputFile("output.txt");
	if(!outputFile){
		cerr << "failed to open output file." << endl;
		return 1;
	}
	
	//spool data to the output file
	for(int i=1; i<=10; i++){
		outputFile << 9 << " x " << i << " = "<< 9*i << endl;
	}
	
	for(int i=1; i<=10; i++){
		outputFile << 10 << " x " << i << " = "<< 10*i << endl;
	}
	
	//close the output filr
	outputFile.close();
	
	//open an input file for spooling
	ifstream inputFile("output.txt");
	if(!inputFile){
		cerr << "Failed to open input file." << endl;
		return 1;
	}
	
	//read and display data from the input file
	string line;
	while (getline(inputFile, line)){
		cout << line << endl;
	}
	
	//close the input file
	inputFile.close();
	return 0;
}
